:rules_000 a void:Dataset;
    void:exampleResource :map_hicGl_000.
:map_hicGl_000 rml:logicalSource :source_000.
:source_000 a rml:LogicalSource;
    rml:source "hic_group_list.csv";
    rml:referenceFormulation ql:CSV.
:map_hicGl_000 a rr:TriplesMap;
    rdfs:label "hicGl".
:s_000 a rr:SubjectMap.
:map_hicGl_000 rr:subjectMap :s_000.
:s_000 rr:template "http://example.com/{group_id}".
:pom_000 a rr:PredicateObjectMap.
:map_hicGl_000 rr:predicateObjectMap :pom_000.
:pm_000 a rr:PredicateMap.
:pom_000 rr:predicateMap :pm_000.
:pm_000 rr:constant ex:HicGroupName.
:pom_000 rr:objectMap :om_000.
:om_000 a rr:ObjectMap;
    rml:reference "group_name";
    rr:termType rr:Literal.
:pom_001 a rr:PredicateObjectMap.
:map_hicGl_000 rr:predicateObjectMap :pom_001.
:pm_001 a rr:PredicateMap.
:pom_001 rr:predicateMap :pm_001.
:pm_001 rr:constant ex:groupType.
:pom_001 rr:objectMap :om_001.
:om_001 a rr:ObjectMap;
    rml:reference "group_type";
    rr:termType rr:Literal.
