:map_vmmGl_000 a rr:TriplesMap;
    rdfs:label "vmmGl".
:s_001 a rr:SubjectMap.
:map_vmmGl_000 rr:subjectMap :s_001.
:s_001 rr:template "http://example.com/{group_id}".
:pom_002 a rr:PredicateObjectMap.
:map_vmmGl_000 rr:predicateObjectMap :pom_002.
:pm_002 a rr:PredicateMap.
:pom_002 rr:predicateMap :pm_002.
:pm_002 rr:constant ex:VmmGroupName.
:pom_002 rr:objectMap :om_002.
:om_002 a rr:ObjectMap;
    rml:reference "group_name";
